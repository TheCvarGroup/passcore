{
    "name": "passcore",
    "version": "1.0.0",
    "description": "PassCore is a self-service password change utility for Active Directory.",
    "main": "index.js",
    "scripts": {
        "lint": "tsc --noEmit && eslint ./**/*.{js,ts,tsx}  --quiet",
        "lint:fix": "eslint ./**/*.{js,ts,tsx}  --quiet --fix",
        "build": "vite build --outDir ../wwwroot",
        "build:tsc": "tsc",
        "prettier": "prettier --check ./**/*.ts*",
        "prettier:fix": "prettier --write ./**/*.ts*",
        "start": "vite --port 4040",
        "dev": "vite --port 4040"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unosquare/passcore.git"
    },
    "author": "Unosquare",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/unosquare/passcore/issues"
    },
    "homepage": "https://github.com/unosquare/passcore",
    "dependencies": {
        "@mui/material": "^6.1.6",
        "@mui/icons-material": "^6.1.6",
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "zxcvbn": "^4.4.2"
    },
    "devDependencies": {
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/zxcvbn": "^4.4.0",
        "@typescript-eslint/eslint-plugin": "^8.15.0",
        "@typescript-eslint/parser": "^8.15.0",
        "@vitejs/plugin-react": "^4.3.3",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "prettier": "^3.3.3",
        "typescript": "^5.6.3",
        "vite": "^6.4.0"
    }
}
